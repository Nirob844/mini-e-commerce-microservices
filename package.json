{
  "name": "mini-e-commerce-microservices",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:gateway": "nest start api-gateway --watch",
    "start:user": "nest start user-service --watch",
    "start:product": "nest start product-service --watch",
    "start:order": "nest start order-service --watch",
    "start:payment": "nest start payment-service --watch",
    "start:auth": "nest start auth-service --watch",
    "start:all": "concurrently \"npm run start:gateway\" \"npm run start:user\" \"npm run start:product\" \"npm run start:order\" \"npm run start:payment\" \"npm run start:auth\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "prisma:generate": "cd apps/user-service && npx prisma generate && cd ../product-service && npx prisma generate && cd ../order-service && npx prisma generate && cd ../payment-service && npx prisma generate && cd ../auth-service && npx prisma generate",
    "prisma:generate:user": "cd apps/user-service && npx prisma generate",
    "prisma:generate:product": "cd apps/product-service && npx prisma generate",
    "prisma:generate:order": "cd apps/order-service && npx prisma generate",
    "prisma:generate:payment": "cd apps/payment-service && npx prisma generate",
    "prisma:generate:auth": "cd apps/auth-service && npx prisma generate",
    "prisma:migrate:user": "cd apps/user-service && npx prisma migrate dev",
    "prisma:migrate:product": "cd apps/product-service && npx prisma migrate dev",
    "prisma:migrate:order": "cd apps/order-service && npx prisma migrate dev",
    "prisma:migrate:payment": "cd apps/payment-service && npx prisma migrate dev",
    "prisma:migrate:auth": "cd apps/auth-service && npx prisma migrate dev",
    "prisma:studio:user": "cd apps/user-service && npx prisma studio --port 5555",
    "prisma:studio:product": "cd apps/product-service && npx prisma studio --port 5556",
    "prisma:studio:order": "cd apps/order-service && npx prisma studio --port 5557",
    "prisma:studio:payment": "cd apps/payment-service && npx prisma studio --port 5558",
    "prisma:studio:auth": "cd apps/auth-service && npx prisma studio --port 5559"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/microservices": "^11.1.12",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/pg": "^8.16.0",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
